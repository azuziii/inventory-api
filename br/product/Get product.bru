meta {
  name: Get product
  type: graphql
  seq: 1
}

post {
  url: http://localhost:3366/graphql
  body: graphql
  auth: none
}

body:graphql {
  query  {
   productResponse(id:"d50c78b1-65e8-4613-ae14-1ba4553cc7e5"){
    product{
      __typename
      ... on Product {
        name
        customer {
          name
          ice
        }
      }
      ... on NotFound {
        code
      }
    }
  }
  }
  
}

