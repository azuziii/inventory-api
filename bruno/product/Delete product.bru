meta {
  name: Delete product
  type: graphql
  seq: 5
}

post {
  url: http://localhost:3366/graphql
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    deleteProductResponse(id: "622e44f4-91e4-495d-965f-5318e2d21cc4") {
      product {
        __typename
        ... on DeleteResponse {
          id
        }
        ... on ProductInUse {
          message
        }
        ... on InvalidData {
          message
        }
      }
    }
  }
  
}
