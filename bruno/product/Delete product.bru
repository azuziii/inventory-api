meta {
  name: Delete product
  type: graphql
  seq: 5
}

post {
  url: http://localhost:3366/graphql
  body: graphql
  auth: none
}

headers {
  x-lang: fr
}

body:graphql {
  mutation {
    deleteProduct(id: "a1eb1aac-0fc3-4446-8f0a-a89852b52a2c") {
      result {
        __typename
        ... on DeleteResponse {
          id
        }
        ... on ProductInUse {
          message
        }
        ... on InvalidData {
          message
        }
      }
    }
  }
  
}
