meta {
  name: Delete customer
  type: graphql
  seq: 5
}

post {
  url: http://localhost:3366/graphql
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    deleteCustomer(id: "8a2edfa9-350c-4c3d-b3e3-2878b229c3e8") {
      result {
        __typename
        ... on DeleteResponse {
          id
        }
        ... on CustomerInUse {
          message
        }
        ... on InvalidData {
          message
        }
      }
    }
  }
  
}
