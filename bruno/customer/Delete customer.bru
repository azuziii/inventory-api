meta {
  name: Delete customer
  type: graphql
  seq: 5
}

post {
  url: http://localhost:3366/graphql
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    deleteCustomerResponse(id: "568c1d4a-426b-4222-9da8-1f3bed6d40dc") {
      customer {
        __typename
        ... on DeleteResponse {
          id
        }
        ... on CustomerInUse {
          message
        }
        ... on InvalidData {
          message
        }
      }
    }
  }
  
}
