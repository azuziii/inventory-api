meta {
  name: Delete customer
  type: graphql
  seq: 5
}

post {
  url: http://localhost:3366/graphql
  body: graphql
  auth: none
}

headers {
  x-lang: fr
}

body:graphql {
  mutation {
    deleteCustomer(id: "e3e165c7-d0f7-456b-895f-170906c35546") {
      result {
        __typename
        ... on DeleteResponse {
          id
        }
        ... on CustomerInUse {
          message
        }
        ... on InvalidData {
          message
        }
      }
    }
  }
  
}
